<h1><%= "#{@user.name}'s Dashboard" %></h1>
<%= button_to "Discover Movies", "/users/#{@user.id}/discover", method: :get %>
<hr>
<% @viewing_parties.each do |party| %>
  <div class="party-card" id="viewing-party-<%= party.id %>">
    <p>Movie Image to GO HERE</p>
    <p><%= party.movie_title %></p>
    <p><%= party.date.strftime("%B %d, %Y") %></p>
    <p><%= party.start_time.strftime("%l:%M %p") %></p>
    <p><%= party.status %></p>
  </div>
<% end %>