<h1>The Movies Discover Page</h1>
<%= button_to "Discover The Top Rated Movies", "/users/#{@user.id}/movies", method: :get, params:{q: 'top 20rated'} %>
<%= form_with url: "/users/#{@user.id}/movies", method: :get, local: true do |form| %>
<%= form.text_field :q %>
<%= form.submit :'Find Movies' %>
<% end %>
